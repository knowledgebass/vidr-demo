# Videos
*.{mp4,[mM][pP][4]} filter=lfs diff=lfs merge=lfs -text
*.{mov,[mM][oO][vV]} filter=lfs diff=lfs merge=lfs -text
*.{wmv,[wW][mM][vV]} filter=lfs diff=lfs merge=lfs -text
*.{mkv,[mM][kK][vV]} filter=lfs diff=lfs merge=lfs -text
*.{avi,[aA][vV][iI]} filter=lfs diff=lfs merge=lfs -text
*.{wemb,[wW][eE][bB][mM]} filter=lfs diff=lfs merge=lfs -text

# Audio
*.{mp3,[mM][pP][3]} filter=lfs diff=lfs merge=lfs -text
*.{m4a,[mM][4][aA]} filter=lfs diff=lfs merge=lfs -text
*.{wav,[wW][aA][vV]} filter=lfs diff=lfs merge=lfs -text

# Images
*.{gif,[gG][iI][fF]} filter=lfs diff=lfs merge=lfs -text
*.{jpg,[jJ][pP][gG]} filter=lfs diff=lfs merge=lfs -text
*.{jpeg,[jJ][pP][eE][gG]} filter=lfs diff=lfs merge=lfs -text
*.{png,[pP][nN][gG]} filter=lfs diff=lfs merge=lfs -text
*.{wemb,[wW][eE][bB][pP]} filter=lfs diff=lfs merge=lfs -text

# Fonts
*.woff2 filter=lfs diff=lfs merge=lfs -text
*.ttf filter=lfs diff=lfs merge=lfs -text
